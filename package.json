    "@adempiere/grpc-dictionary-client": "^1.3.5",
    "@adempiere/grpc-enrollment-client": "^1.0.7",
    "autoprefixer": "^9.5.1",
    "axios": "0.18.0",
    "clipboard": "2.0.4",
